{

  "description": "Princess Princess Copy.",
  "manifest_version": 2,
  "name": "prsprscopy",
  "version": "0.1.0",
  "icons": {
    "48": "icons/icon-48.png"
  },

  "background": {
    "scripts": [
      "background/background.js"
    ]
  },

  "commands": {
    "PrsPrsCopy": {
      "suggested_key": {
        "default": "Alt+C"
      },
      "description": "Addon's Copy key."
    }
  },

  "options_ui": {
    "page": "options_ui/options.html"
  },

  "permissions":[
    "activeTab",
    "clipboardWrite",
    "storage",
    "tabs",
    "<all_urls>"
  ]

}
